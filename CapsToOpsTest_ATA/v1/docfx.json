{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*_themes/**"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "dest": "_site",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/breadcrumb.json"
    }
  },
  "render": {
    "theme": "hyde",
    "dest": "_output"
  },
  "basepath": "/ATA"
}
